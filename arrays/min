#include<iostream>
#include<climits>
#include<vector>
using namespace std;
int main(){
        int n;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++){
            cin>>arr[i];
        }
        int min=INT_MAX;
        for(auto &x:arr){
            if(x<min){
                min=x;
            }
        }
        cout<<"min is "<<min;
}
